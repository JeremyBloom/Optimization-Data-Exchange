{
  "Problem": {
    "Name": "warehouse_location",
    "Reference": "https://github.com/JeremyBloom/Optimization---Sample-Notebooks/blob/master/Optimization%2BModeling%2Band%2BRelational%2BData%2Bpub.ipynb",
    "Author": "Jeremy A. Bloom (jeremyblmca@gmail.com)",
    "Notices": "Copyright Â© 2019 Jeremy A. Bloom"
  },
  "Input_Data_Model": {
    "warehouses": {
      "*location":    "String",
      "fixedCost":    "Double",
      "capacityCost": "Double"
    },
    "routes": {
      "*location":    "String",
      "*store":       "String",
      "shippingCost": "Double"
    },
    "stores": {
      "*store":       "String"
    },
    "demands": {
      "*store":       "String",
      "amount":       "Double"
    }
},
  "Output_Data_Model": {
    "objectives": {
      "*problem":     "String",
        "*dExpr":     "String",
        "value":      "Double"
    },
    "openWarehouses": {
        "*location":  "String",
        "open":       "Integer",
        "capacity":   "Double"
    },
    "shipments": {
        "*location":  "String",
        "*store":     "String",
        "amount":     "Double"
    }
  },
  "Data": {
    "routes" : [
      {"location" : "Brockton, MA", "store" : "Malden, MA", "shippingCost" : 42.94},
  //  	...
      {"location" : "Bristol, CT", "store" : "Stamford, CT", "shippingCost" : 94.66}
    ],
    "stores" : [
      {"storeId" : "Malden, MA"},
  //  	...
      {"storeId" : "Stamford, CT"}
    ],
    "warehouses" : [
      {"location" : "Brockton, MA", "fixedCost" : 550000.0, "capacityCost" : 148.0},
      {"location" : "Bristol, CT", "fixedCost" : 600000.0, "capacityCost" : 148.0}
    ],
    "demands" : [
      {"store" : "Malden, MA", "scenarioId" : "0", "amount" : 104.0},
  //  	...
      {"store" : "Stamford, CT", "scenarioId" : "0", "amount" : 73.0}
    ]
  },
  "Variables": [
    {
      "Name": "open",
      "Type": "Binary",
      "Index": "warehouses",
      "Bounds": "Non-negative"
    },
    {
      "Name": "capacity",
      "Type": "Continuous",
      "Index": "warehouses",
      "Bounds": "Non-negative"
    },
    {
      "Name": "ship",
      "Type": "Continuous",
      "Index": "routes",
      "Bounds": {"Lower": 0.0, "Upper": 1.0}
    }
  ],
  "Constraints": [
    {
      "Name": "ctCapacity",
      "Type": "Linear",
      "Index": "warehouses",
      "Sense": ">=",
      "RHS": 0.0
    },
    {
      "Name": "ctDemand",
      "Type": "Linear",
      "Index": "stores",
      "Sense": ">=",
      "RHS": 1.0
    },
    {
      "Name": "ctSupply",
      "Type": "Linear",
      "Index": "routes",
      "Sense": "<=",
      "RHS": 0.0
    }
  ],
  "Decision_Expressions": [
    {
      "Name": "fixedCost",
      "Type": "Linear",
      "Index": "Self",
      "Sense": "None"
    },
    {
      "Name": "variableCost",
      "Type": "Linear",
      "Index": "Self",
      "Sense": "None"
    },
    {
      "Name": "totalCost",
      "Type": "Linear",
      "Index": "Self",
      "Sense": "Minimize"
    }
  ],
  "Coefficients": [
    {
      "Name": "entries_fixed_open",
      "Decision_Expressions": "fixedCost",
      "Variables": "open",
      "Entries": "warehouses.fixedCost"
    },
    {
      "Name": "entries_fixed_capacity",
      "Decision_Expressions": "fixedCost",
      "Variables": "capacity",
      "Entries": "warehouses.capacityCost"
    },
    {
      "Name": "entries_operating_ship",
      "Decision_Expressions": "operatingCost",
      "Variables": "ship",
      "Entries": "routes.shippingCost"
    },
    {
      "Name": "entries_total_fixed",
      "Decision_Expressions": "totalCost",
      "Variables": "fixedCost",
      "Entries": 1.0
    },
    {
      "Name": "entries_total_operating",
      "Decision_Expressions": "totalCost",
      "Variables": "variableCost",
      "Entries": 1.0
    },
    {
      "Name": "entries_ctCapacity_capacity",
      "Constraints": "ctCapacity",
      "Variables": "capacity",
      "Entries": 1.0
    },
    {
      "Name": "entries_ctCapacity_ship",
      "Constraints": "ctCapacity",
      "Variables": "ship",
      "Entries": "-demands.amount"
    },
     {
      "Name": "entries_ctDemand_ship",
      "Constraints": "ctDemand",
      "Variables": "ship",
      "Entries": 1.0
    },
     {
      "Name": "entries_ctSupply_open",
      "Constraints": "ctSupply",
      "Variables": "open",
      "Entries": -1.0
    },
     {
      "Name":"entries_ctSupply_ship",
      "Constraints": "ctSupply",
      "Variables": "ship",
      "Entries": 1.0
    }
  ]
}